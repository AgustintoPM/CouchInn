<%= form_for(@hospedaje) do |f| %>
	<%=	render 'error_messages' %>
    <%= f.label	:Titulo %>
    <%= f.text_field :titulo %>
    <%= f.label :foto%>
    <%= f.text_field :foto %>
    <%= f.label :lugar %>
    <%= f.text_field :lugar %>
    <%= f.label :fecha %>
    <%= f.text_field :fecha %>
	<%= f.label :capacidad %>
    <%= f.text_field :capacidad %>
	<%= f.submit "Guardar", class: "btn btn-primary" %>
<% end %>
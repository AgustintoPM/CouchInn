<div class="row">
 <div class="col-md-6 col-md-offset-3">
    <%= form_for(@hospedaje)	do |f| %>
    <%= f.label	:Titulo %>
    <%= f.text_field :titulo %>
    <%= f.label :foto%>
    <%= f.text_field :foto %>
    <%= f.label :lugar %>
    <%= f.text_field :lugar %>
    <%= f.label :fecha %>
    <%= f.text_field :fecha %>
	<%= f.label :capacidad %>
    <%= f.text_field :capacidad %>
    <%= f.submit "Publicar mi hospedaje", class: "btn btn-primary" %>
    <% end %>
 </div>
</div>
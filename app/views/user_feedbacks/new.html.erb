<% provide(:title, 'Calificar') %>
<h1>Complete los campos con los datos solicitados</h1>

<div class="row">
 <div class="col-md-6 col-md-offset-3">
    <%= form_for(@user_feedback)	do |f| %>
    
  
    <%= f.hidden_field :destination, :value => params[:format]%>
    <%= f.label	:descripcion %>
    <%= f.text_field :content %>
    <%= f.label :puntaje %>
    
    <%= f.select(:score, options_for_select([[1],[2],[3],[4],[5]], selected: :score )) %>
    <%= f.submit "Calificar", class: "btn btn-primary" %>
    <% end %>
 </div>
</div>
<% provide(:title, "Mejorar cuenta")	%>
<h1>Actualice su cuenta a premium</h1>
<div class="row">
		<div class="col-md-6 col-md-offset-3">
				<%= form_for(@user) do |f| %>
						<%=	render 'shared/error_messages'	%>
						<%=	f.label :Tarjeta_de_credito %>
						<%=	f.text_field :credit_card, class: 'form-control' %>
						<%=	f.submit "Guardar cambios", class: "btn btn-primary" %>
				<% end %>
		</div>
</div>